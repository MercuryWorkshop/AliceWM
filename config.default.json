{
    "apps": [
        "apps/term.app",
        "apps/fsapp.app",
        "apps/marketplace.app",
        "apps/taskbarMan.app",
        "apps/ashell.app"
    ],
    "libs": [
        "/apps/libfileview.lib",
        "/apps/libfilepicker.lib",
        "/apps/libstore.lib",
        "/apps/libpersist.lib",
        "/apps/libphoenix.lib"
    ],
    "defaultsettings": {
        "use-sw-cache": false,
        "applist": ["anura.browser", "anura.settings", "anura.fsapp"],
        "clampWindows": true,
